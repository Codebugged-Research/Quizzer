<!DOCTYPE html>
<html lang="en">

<head>
    <%- include("partials/head") %>
</head>

<body>
    <header>
        <%- include("partials/header") %>
    </header>
    <br />
    <div class="container">
        <div class="jumbotron">
            <div class="row-lg-12 d-flex justify-content-center">
                <div class="col-lg-12 sm-4 md-8">
                    <table class="table">
                        <h6><strong></strong>
                            <h2>Questions</h2>
                        </h6><br>
                        <thead class="thead-dark">
                            <tr>
                                <th scope="col">S.No</th>
                                <th scope="col">Question</th>
                                <th scope="col">Date</th>
                                <th scope="col">Duration</th>
                                <th scope="col">Status</th>
                            </tr>
                        </thead>
                        <tbody>
                            <% questions.forEach(function(question,index){ %>
                                <tr>
                                    <td>
                                        <%= index+1 %>
                                    </td>
                                    <td>
                                        <%= question.questionid %>
                                    </td>
                                    <td>
                                        <%= question.question_day %>
                                    </td>
                                    <td>
                                        <% questions.forEach(function(question){%>
                                            <!--To be filled with question properties-->
                                            <% <br>
                                                    <% })%>
                                    </td>
                                    <td>
                                        <%= order.address %>
                                    </td>
                                    <td>
                                        <%= order.total %>
                                    </td>
                                    <td>
                                        <%= question.questionStatus %>
                                    </td>
                                    <td>
                                        <a href="/admin-question-details/<%= question.questionid %>"><button type="button" class="btn btn-sm btn-warning">Details</button></a>
                                    </td>
                                </tr>
                                <% })%>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
    <script src="https://code.jquery.com/jquery-3.5.1.slim.min.js" integrity="sha384-DfXdz2htPH0lsSSs5nCTpuj/zy4C+OGpamoFVy38MVBnE+IbbVYUew+OrCXaRkfj" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js" integrity="sha384-Q6E9RHvbIyZFJoft+2mJbHaEWldlvI9IOYy5n3zV9zzTtmI3UksdQRVvoxMfooAo" crossorigin="anonymous"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.5.0/js/bootstrap.min.js" integrity="sha384-OgVRvuATP1z7JjHLkuOU7Xw704+h835Lr+6QL9UvYjZE3Ipu6Tp75j7Bh/kR0JKI" crossorigin="anonymous"></script>
</body>

</html>